<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
	<display-name>PiInjector</display-name>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>TestNew</servlet-name>
		<servlet-class>main.java.servlet.TestNew</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestNew</servlet-name>
		<url-pattern>/test-new-form</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TestDisplay</servlet-name>
		<servlet-class>main.java.servlet.TestDisplay</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestDisplay</servlet-name>
		<url-pattern>/test-display-form</url-pattern>
	</servlet-mapping>
</web-app>